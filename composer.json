{
  "name": "pingecom/l5-tabler",
  "description": "Tabler template for Laravel 5.*",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "baorv",
      "email": "roanvanbao@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "tabler/tabler": "^0.0.32",
    "illuminate/support": "~5.5",
    "illuminate/auth": "~5.5",
    "lavary/laravel-menu": "^1.7",
    "laravelcollective/html":"~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0",
    "orchestra/testbench": ">=3.5"
  },
  "autoload": {
    "psr-4": {
      "Pingecom\\Tabler\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Pingecom\\Tabler\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pingecom\\Tabler\\Providers\\TablerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
